<!DOCTYPE html>
<html>
<head>
  <title>GoalsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<h4><%= current_user.username if current_user %></h4>

<% flash[:errors].try(:each) do |error| %>
  <%= error %>
<% end %>

<%= yield %>

</body>
</html>
