<!DOCTYPE html>
<html>
<head>
  <title>GoalsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
<h4><%= current_user.username %></h4>
<%= button_to "Log Out", session_url, method: :delete %>
<% end %>

<% flash[:errors].try(:each) do |error| %>
  <%= error %>
<% end %>

<%= yield %>

</body>
</html>
